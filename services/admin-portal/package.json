{
  "name": "astradesk-admin",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "eslint . --max-warnings=0",
    "format": "prettier --check \"**/*.{ts,tsx,js,json,md}\"",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,json,md}\"",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "openapi:gen": "tsx scripts/openapi-generate.ts",
    "openapi:check": "tsx scripts/check-openapi-sync.ts",
    "api:generate": "tsx scripts/openapi-generate.ts"
  },
  "dependencies": {
    "@headlessui/react": "2.1.5",
    "clsx": "2.1.1",
    "next": "^15.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "valibot": "0.36.0"
  },
  "devDependencies": {
    "@types/node": "^22.18.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "7.15.0",
    "@typescript-eslint/parser": "7.15.0",
    "autoprefixer": "10.4.20",
    "eslint": "8.57.1",
    "eslint-config-next": "^15.0.0",
    "eslint-config-prettier": "9.1.0",
    "openapi-typescript": "^7.10.1",
    "postcss": "8.4.47",
    "prettier": "3.3.3",
    "tailwindcss": "3.4.13",
    "tsx": "^4.20.6",
    "typescript": "5.6.2",
    "vite-tsconfig-paths": "4.3.1",
    "vitest": "^3.2.4"
  }
}
